  "scripts": {
    "build": "next build",
    "start": "next start",
    "dev": "next dev",
    "lint": "eslint . --ext .ts,.tsx",
    "format": "prettier --write \"src/**/*.{ts,tsx}\"",
    "clean": "rimraf dist",
    "instagram": "ts-node src/instagram.ts",
    "instagram-fetch": "ts-node src/run-instagram.ts",
    "followings": "ts-node src/get-followings.ts",
    "detect-events": "ts-node src/event-detector.ts",
    "detect-events-ts": "ts-node --transpile-only src/event-detector.ts",
    "scheduler": "ts-node src/scheduler.ts",
    "dashboard": "next dev",
    "dashboard:build": "next build",
    "dashboard:start": "next start",
    "start-all": "./start-scheduler.sh && ./start-dashboard.sh",
    "stop-all": "./stop-scheduler.sh && ./stop-dashboard.sh"
  },
  "keywords": [
    "typescript",
    "node"
  ],
  "author": "",
  "license": "ISC",
