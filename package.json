    "autoprefixer": "10.4.15",
    "eslint": "9.22.0",
    "eslint-config-prettier": "10.1.1",
    "postcss": "8.4.31",
    "postcss-import": "16.1.0",
    "prettier": "3.5.3",
    "rimraf": "6.0.1",
    "tailwindcss": "3.3.0",
    "ts-node": "10.9.2",
    "ts-node-dev": "2.0.0",
